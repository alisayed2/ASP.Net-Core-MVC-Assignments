@model TraineeResultVM
@{
    ViewData["Title"] = "TraineeCourseStatus";
}

<h1>TraineeCourseStatus</h1>

<table class="table">
    <thead>
        <tr>
            <th scope="col">Trainee Name</th>
            <th scope="col">Course Name</th>
            <th scope="col">Degree</th>
        </tr>
    </thead>
    <tbody>
            <tr>
                <th scope="col">@Model.TraineeName</th>
                <th scope="col">@Model.CourseName</th>
                <th scope="col" style="color:@Model.Color">@Model.Degree</th>
            </tr>
    </tbody>

