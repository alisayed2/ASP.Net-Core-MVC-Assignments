@model List<TraineeResultVM>
@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<table class="table">
    <thead>
        <tr>
            <th scope="col">Trainee Name</th>
            <th scope="col">Course Name</th>
            <th scope="col">Degree</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {

            <tr>
                <th scope="col">@item.TraineeName</th>
                <th scope="col">@item.CourseName</th>
                <th scope="col">@item.Degree</th>
            </tr>
        }
    </tbody>
</table>


